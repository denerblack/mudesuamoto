<h1>Listing years</h1>

<table>
  <tr>
    <th>Year</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @years.each do |year| %>
  <tr>
    <td><%= year.year %></td>
    <td><%= link_to 'Show', admin_year_path(year) %></td>
    <td><%= link_to 'Edit', edit_admin_year_path(year) %></td>
    <td><%= link_to 'Destroy', admin_year_path(year), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Year', new_admin_year_path %>

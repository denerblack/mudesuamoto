<h1>Listing admin_parts</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @parts.each do |part| %>
  <tr>
    <td><%= admin_part.name %></td>
    <td><%= admin_part.description %></td>
    <td><%= link_to 'Show', admin_part_path(part) %></td>
    <td><%= link_to 'Edit', edit_admin_part_path(part) %></td>
    <td><%= link_to 'Destroy', admin_part_path(part), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

